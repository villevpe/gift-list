{
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": "off",
        "no-console": "off",
        "no-use-before-define": [
            "error",
            {
                "functions": false,
                "classes": true
            }
        ],
        "max-len": [
            "error",
            120
        ],
        "comma-dangle": "off",
        "import/no-extraneous-dependencies": [2, { "devDependencies": true }]
    }
}